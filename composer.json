{
    "name": "tnmdev/ussd",
    "description": "TruRoute USSD Adaptor for Laravel PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TNM Developers",
            "email": "developers@tnm.co.mw"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": ">=5.8",
        "ext-json": "*",
        "ext-simplexml": "*",
        "tnmdev/ussd-simulator": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "TNM\\USSD\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TNM\\USSD\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TNM\\USSD\\UssdServiceProvider"
            ]
        }
    }
}
